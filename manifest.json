{
  "manifest_version": "0.3",
  "name": "redactor-mcp",
  "display_name": "Redactor MCP",
  "version": "0.1.0",
  "description": "MCP server for AWS Comprehend PII detection and redaction",
  "author": {
    "name": "Jarrett Meyer",
    "email": "jarrettmeyer@gmail.com"
  },
  "server": {
    "type": "node",
    "entry_point": "dist/index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/dist/index.js"]
    }
  },
  "compatibility": {
    "platforms": ["darwin", "win32", "linux"]
  }
}
